  p_codes :
   -
     code : |1-
      LOCAL 
      	MonFlux est un rssFlux
      	MonCanal est un rssCanal
      	MonEntrée est un rssEntrée
      	
      	MonFlux.Version = "2.0"
      
      	MonCanal..Titre = LitParamètreSite(CMS_TITRE_FLUX_RSS)
      	MonCanal..Description = LitParamètreSite(CMS_DESCRIPTION_FLUX_RSS)
      	MonCanal..DateMiseAJour = DateSys() + HeureSys()
      
      	// Renseignements du canal
      	tabArticle est un tableau d'entiers sur 8 octets = CArticle::s_ListeRecents(20)
      	clArticle est un CArticle
      	POUR TOUT nIDArticle DE tabArticle
      		SI clArticle.bChargeInfo(nIDArticle) ALORS
      		
      			// Renseignements des entrées du canal 1
      			MonEntrée..Auteur = clArticle.NomAuteur()
      			MonEntrée..Description = clArticle.m_sRésuméArticle
      			MonEntrée..Titre = clArticle.m_sTitreArticle
      			MonEntrée..DatePublication = clArticle.m_dhDatePublication
      			MonEntrée..Catégorie.Valeur = clArticle.NomCatégorie()
      						
      			// Ajoute l'entrée dans le canal
      			Ajoute(MonCanal..Entrée,MonEntrée)
      			
      		FIN
      	
      	FIN	
      	// Ajoute le canal dans le flux
      	Ajoute(MonFlux..Canal,MonCanal)
      
      
      rssAffiche(MonFlux)
