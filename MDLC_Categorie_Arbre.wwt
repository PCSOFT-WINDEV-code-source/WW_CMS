  procedures :
   -
     name : _RemplirArbreCatégorie
     procedure_id : 1586403109238116104
     type_code : 14
     code : |1-
      // Résumé : Remplit une branche de l'arbre des catégories et se rappelle récursivement
      // Syntaxe :
      //_RemplirArbreCatégorie (<nIDParent> est entier sur 8 octets, <sCheminEnCours> est chaîne)
      //
      // Paramètres :
      //	nIDParent (entier sur 8 octets) :Identifiant de la branche courante
      //	sCheminEnCours (chaîne ANSI) :<indiquez ici le rôle de sCheminEnCours>
      // Valeur de retour :
      // 	Aucune
      //
      PROCÉDURE PRIVÉE _RemplirArbreCatégorie( nIDParent est un entier sur 8, sCheminEnCours est une chaîne )
      
      tabCategorie est un tableau d'entiers sur 8 octets = CCategorie::s_ListeEnfants(nIDParent)
      clCateg est un CCategorie
      nIDCatagorie est un entier sur 8
      POUR TOUT nIDCatagorie DE tabCategorie 
      	// Ajout d'une catégorie
      	SI clCateg.bChargeInfo(nIDCatagorie) ALORS
      		sChemin est une chaîne = sCheminEnCours + [TAB] + clCateg.m_sLibelléCatégorie
      		
      		// CheminImage retourne une URL absolue, il faut fournir une URL relative à ArbreAjoute
      		// on doit donc calculer l'URL relative
      		sImage est une chaîne = clCateg.CheminImage()
      		sURLRelativeImage est une chaîne = sImage
      		SI sImage<>"" ALORS
      			sURLRelativeImage = sImage[[Taille(COL_UtilitaireSite.gsBaseURLSite)+1 À ]]
      		FIN
      
      		ArbreAjoute(ARBRE_Catégories,sChemin,(sImage<>"" ? sURLRelativeImage SINON aDéfaut),(sImage<>"" ? sURLRelativeImage SINON aDéfaut),nIDCatagorie,aDernier,clCateg.m_sDescriptionCatégorie)
      		
      		
      		
      		taParam est un tableau associatif de chaînes = [ [ "id", nIDCatagorie ] ]
      		SI gCallbackClicCategorie=Null ALORS
      			ARBRE_Catégories[sChemin]..URL = ConstruitURL(pageadresse(categorie),taParam,Vrai)
      		FIN
      		
      		// Appel récursif
      		_RemplirArbreCatégorie(nIDCatagorie,sChemin)
      	FIN
      FIN
     type : 458752
   -
     name : RemplirArbreCategorie
     procedure_id : 1586691838364701716
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //RemplirArbreCategorie (<nIDBase> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDBase (entier sur 8 octets) : <indiquez ici le rôle de nIDBase>
      // Valeur de retour :
      // 	Aucune
      //
      PROCÉDURE RemplirArbreCategorie( nIDBase est un entier sur 8 octets )
      
      ArbreSupprimeTout(ARBRE_Catégories)
      
      _RemplirArbreCatégorie(nIDBase,"")
      
      LIB_Catégories..Visible = (ARBRE_Catégories..Occurrence>0)
      
     type : 458752
