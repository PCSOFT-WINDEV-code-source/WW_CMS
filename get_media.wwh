  type_code : 38
  p_codes :
   -
     code : |1-
      
     type : 851968
   -
     code : |1+
      nLargeur est un entier = PageParamètre("larg")
      nHauteur est un entier = PageParamètre("haut")
      
      // Lecture d'une image du fichier media
      SI HLitRecherchePremier(Media,IDMedia,PageParamètre("id")) ALORS
      	img est une Image
      	img.Image = Media.ContenuMedia
      
      	SI nLargeur<>0 _OU_ nHauteur<>0 ALORS
      		SI nLargeur>0 _ET_ nHauteur=0 ALORS
      			// Largeur spécifiée - hauteur homothétique
      			nHauteur = nLargeur*img.Hauteur / img.Largeur
      			dRedimensionne(img,nLargeur,nHauteur,drHomothétique)
      		SINON
      			SI nLargeur=0 _ET_ nHauteur>0 ALORS
      				// Hauteur spécifiée - largeur homothétique
      				nLargeur = nHauteur*img.Largeur / img.Hauteur
      				dRedimensionne(img,nLargeur,nHauteur,drHomothétique)
      			SINON
      				// Largeur et hauteur spécifiées
      				dRedimensionne(img,nLargeur,nHauteur,drHomothétiqueCentré)
      			FIN
      		FIN
      	FIN
      
      	ChaîneAffiche(dSauveImagePNG(img,enMémoire),"image/png",Media.TitreMedia+".png")
      FIN	
      
      
      
      
